+void OutdoorPvPCA::CheckNPC()
+{
+	for (CreatureSet::iterator itr = m_creature.begin(); itr != m_creature.end(); ++itr)
+	{
+		if((*itr)->isDead())
+		{
+			for(int i = 0; i < CA_TOWER_NUM; ++i)
+			{
+				if((*itr)->GetEntry() == CA_NPC[i].entry && check_towers[i] == 0)
